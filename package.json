{
  "name": "react-netlify-identity-widget",
  "version": "0.0.2",
  "main": "dist/index.js",
  "umd:main": "dist/reactNetlifyIdentityWidget.umd.production.js",
  "module": "dist/reactNetlifyIdentityWidget.es.production.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
