  "devDependencies": {
    "@types/jest": "^24.0.11",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "auto-changelog": "^1.13.0",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "2.1.0",
    "gh-release": "^3.5.0",
    "husky": "^1.3.1",
    "tsdx": "^0.3.4",
    "typescript": "^3.4.4"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "@reach/dialog": "^0.2.3",
    "@reach/visually-hidden": "^0.1.3",
    "react-netlify-identity": "^0.0.7"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
